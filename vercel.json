{
  "version": 2,
  "name": "read-it-now",
  "framework": null,
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node",
      "includeFiles": ["shared/**"]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}