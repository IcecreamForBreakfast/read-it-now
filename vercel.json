{
  "version": 2,
  "name": "read-it-now",
  "buildCommand": "npm run build && cp -r dist/public/* dist/",
  "outputDirectory": "dist",
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}